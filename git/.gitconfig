[user]
	email = <mail>
	name = <name>

[push]
	default = simple

[alias]
  s = status
	lg = log --all --graph --pretty='%h %C(cyan)%cd %C(auto)%d %C(bold yellow)%an%n%C(reset)%<(139,trunc)%s%n'

[merge]
    tool = kdiff3

[mergetool "kdiff3"]
    path = /usr/local/bin/kdiff3

[diff]
    tool = kdiff3

[difftool "kdiff3"]
    path = /usr/local/bin/kdiff3
